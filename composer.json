{
    "name": "mar4ehk0/ocr-bundle",
    "version": "0.1.0",
    "type": "symfony-bundle",
    "description": "Bundle for text recognition",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^4.1",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "mar4ehk0/base_entity-bundle": "^0.1.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "mar4ehk0\\OCRBundle\\": "src/"
        }
    },
    "scripts": {
        "test" : "./vendor/bin/simple-phpunit"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "repositories": [
        {
            "type" : "path",
            "url" : "../BaseEntityBundle"
        }
    ],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64"
    }
}
